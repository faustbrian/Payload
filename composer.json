{
    "name": "faustbrian/payload",
    "description": "Serialisers/Deserialisers for several data-types",
    "keywords": ["payload", "Payload", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.1",
        "league/csv": "^8.0",
        "netresearch/jsonmapper": "^1.00",
        "symfony/property-access": "^3.1",
        "symfony/serializer": "^3.1",
        "symfony/yaml": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Payload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Payload\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
